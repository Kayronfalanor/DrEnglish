							<% End If %>

							</td>
						</tr>

						<%
						next
							function randNumChoice()
							  Dim RandNum                 : RandNum = ""
							  Dim coupTemp                : coupTemp=""
							  Dim couponValue(35)

							  couponValue(0)   = "A"
							  couponValue(1)   = "B"
							  couponValue(2)   = "C"
							  couponValue(3)   = "D"
							  couponValue(4)   = "E"
							  couponValue(5)   = "F"
							  couponValue(6)   = "G"
