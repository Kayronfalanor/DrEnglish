							  couponValue(29)  = "3"
							  couponValue(30)  = "4"
							  couponValue(31)  = "5"
							  couponValue(32)  = "6"
							  couponValue(33)  = "7"
							  couponValue(34)  = "8"
							  couponValue(35)  = "9"

							  Do while true
								Randomize
								coupTemp = couponValue(Int((35- 0 + 1) * Rnd + 0))
								if instr(RandNum, coupTemp) = 0 then
									RandNum = RandNum & coupTemp
									if Len(RandNum) = 4 then
									  Exit Do
									End If
								End If
							  Loop
							  randNumChoice = RandNum
							End Function
